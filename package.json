{
  "name": "marvel-shuffle",
  "version": "3.7.7",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "node build.js hashes assets",
    "deck": "node build.js deck",
    "deck:force": "node build.js --force deck",
    "images": "node build.js images hashes",
    "images:force": "node build.js --force images hashes",
    "images:hashes": "node build.js hashes",
    "release": "npm run release:patch",
    "release:major": "node release.js major",
    "release:minor": "node release.js minor",
    "release:patch": "node release.js patch",
    "start": "serve docs"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-css-order",
      "prettier-plugin-packagejson",
      "prettier-plugin-organize-imports"
    ]
  },
  "dependencies": {
    "serve": "14.2.5"
  },
  "devDependencies": {
    "axios": "1.12.0",
    "glob": "11.0.3",
    "image-size": "2.0.2",
    "prettier": "3.6.2",
    "prettier-plugin-css-order": "2.1.2",
    "prettier-plugin-organize-imports": "4.2.0",
    "prettier-plugin-packagejson": "2.5.19",
    "replace-in-file": "8.3.0"
  }
}
