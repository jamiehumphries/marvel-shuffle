{
  "name": "marvel-shuffle",
  "version": "2.5.1",
  "private": true,
  "type": "module",
  "scripts": {
    "assets": "node assets.js",
    "build": "npm run build:patch",
    "build:major": "node build.js --major",
    "build:minor": "node build.js --minor",
    "build:patch": "node build.js --patch",
    "images": "node images.js",
    "round": "node images.js --no-level",
    "start": "serve docs"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-css-order",
      "prettier-plugin-packagejson"
    ]
  },
  "dependencies": {
    "serve": "14.2.4"
  },
  "devDependencies": {
    "glob": "11.0.0",
    "prettier": "3.3.3",
    "prettier-plugin-css-order": "2.1.2",
    "prettier-plugin-packagejson": "2.5.6",
    "replace-in-file": "8.2.0"
  }
}
